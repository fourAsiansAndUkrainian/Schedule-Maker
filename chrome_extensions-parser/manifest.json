{
  "manifest_version": 2,

  "name": "Catman",
  "description": "My Thing Doesn't Work ",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon1.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "tabs","activeTab","clipboardRead","clipboardWrite"
   ],
  "background": {
   "scripts":["background.js"]
  },
  "content_scripts": [{
   "matches": ["file:///*"],
   "js":["content.js"]
  }]
}